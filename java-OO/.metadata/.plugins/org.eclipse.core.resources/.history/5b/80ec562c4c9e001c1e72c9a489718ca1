
public class TestaCliente {

	public static void main(String[] args) {
		Cliente pedro = new Cliente();
		Conta contaDoPedro = new Conta();
		
		pedro.nome = "Pedro";
		pedro.cpf = "342.377.578-54";
		pedro.profissao = "programador";
		
		contaDoPedro.titular = pedro;
		
		System.out.println(contaDoPedro.titular.nome);
		
		Conta contaDaHinara = new Conta();
		contaDaHinara.titular = new Cliente();
		contaDaHinara.titular.nome = "Hinara";
		
		System.out.println(contaDaHinara.titular);
		System.out.println(contaDaHinara.titular.nome);
	}
}
